    ipLabelImage : _ITcVnImage;
    ipBoundingBoxes : _ITcVnContainer;
    ipNumPixels : _ITcVnContainer;
    ipCentroids : _ITcVnContainer;
    nLabels : _DINT(0);
    hrPrev : _HRESULT(S_OK);
	hresult : _HRESULT(S_OK);
END_VAR
]]></Declaration>
    <Implementation>
