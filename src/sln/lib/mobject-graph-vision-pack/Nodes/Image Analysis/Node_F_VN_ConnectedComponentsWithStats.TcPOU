    <Method Name="Clone" Id="{e8de6de8-25ae-02e1-2fb2-366e8c0b87e7}">
      <Declaration><![CDATA[METHOD PUBLIC  Clone : I_Node
VAR
	pNode : POINTER TO Node_F_VN_ConnectedComponentsWithStats;
END_VAR]]></Declaration>
      <Implementation>
        <ST><![CDATA[pNode := __NEW(Node_F_VN_ConnectedComponentsWithStats);
Clone := pNode^;]]></ST>
      </Implementation>
    </Method>
    <Method Name="FB_init" Id="{098b4084-ddd4-073e-0336-579b31c3550d}" FolderPath="Constructor\">
      <Declaration><![CDATA[METHOD FB_init : BOOL
VAR_INPUT
	bInitRetains : BOOL; // if TRUE, the retain variables are initialized (warm start / cold start)
	bInCopyCode : BOOL;  // if TRUE, the instance afterwards gets moved into the copy code (online change)
END_VAR
]]></Declaration>
      <Implementation>
        <ST><![CDATA[AddInput('ipSrcImage', ipSrcImage);
AddInput('hrPrev', hrPrev);
AddOutput('ipLabelImage', ipLabelImage);
AddOutput('ipBoundingBoxes', ipBoundingBoxes);
AddOutput('ipNumPixels', ipNumPixels);
AddOutput('ipCentroids', ipCentroids);
