VAR
	localData : ETcVnSignedNormalization;
	activeData : REFERENCE TO ETcVnSignedNormalization;
	enumBaseDatatype : _INT(localData);
END_VAR
]]></Declaration>
    <Implementation>
      <ST><![CDATA[]]></ST>
    </Implementation>
